{
  "name": "nsedrickm.github.io",
  "version": "1.0.0",
  "description": "My little empire",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html",
    "build": "rm -rf ./dist/* && parcel build index.html",
    "clean": "rm -rf ./dist/* "
  },
  "keywords": [
    "nsedrickm",
    "portfolio"
  ],
  "author": "nSedrickm",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta1",
    "devicon": "^2.2.0",
    "glightbox": "^3.0.7"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "sass": "^1.32.2"
  },
  "customDistStructure": {
    "config": {
      "js": [
        ".js",
        ".js.map"
      ],
      "css": [
        ".css",
        ".css.map"
      ],
      "images": [
        ".jpg",
        ".png",
        ".webp"
      ],
      "fonts": [
        ".ttf",
        ".svg",
        ".eot",
        ".woff",
        ".woff2"
      ]
    },
    "options": {
      "development": true
    }
  }
}
